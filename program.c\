#include <stdio.h>
#include <stdlib.h>
#include <pthread.h>
#include <stdint.h>


void* calculate(void* args){

	int print=(int)(intptr_t)args;
	printf("\nFrom Main:%d\n",print);
	return NULL;

}


int main(){

pthread_t t_id;
int a=90;
void* args;

pthread_create(&t_id,NULL,calculate,(void *)(intptr_t)a);

pthread_join(t_id,NULL);


int num1=23;
int num2=24;
int sum;

__asm__(

"movl %1,%%eax\n"
"addl %2,%%eax\n"
"movl %%eax,%0"
:"=r"(sum)
:"r"(num1),"r"(num2)
:"%eax"

);


printf("sum of %d and %d is: %d\n",num1,num2,sum);
return 0;

}
